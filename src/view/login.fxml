         <rotationAxis>
            <Point3D />
         </rotationAxis>
      </ImageView>
      <Label layoutX="101.0" layoutY="275.0" text="ID">
         <font>
            <Font size="24.0" />
         </font></Label>
      <TextField fx:id="txtId" layoutX="144.0" layoutY="275.0" style="-fx-background-color: #CED8F6;" />
      <Label layoutX="75.0" layoutY="315.0" text="PWD">
         <font>
            <Font size="24.0" />
         </font></Label>
      <PasswordField fx:id="txtPassword" layoutX="143.0" layoutY="314.0" style="-fx-background-color: #CED8F6;" />
      <Button fx:id="btnLogin" layoutX="142.0" layoutY="358.0" mnemonicParsing="false" style="-fx-background-color: white;" text="Login">
